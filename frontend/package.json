{
    "name": "job-automation-frontend",
    "version": "1.0.0",
    "description": "Frontend for Job Application Automation Platform",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "test": "vitest --run",
